cmake_minimum_required(VERSION @CMAKE_MINIMUM_REQUIRED_VERSION@)
project(@DEPENDENCY@)

include(ExternalProject)

ExternalProject_Add(
  @DEPENDENCY@
  GIT_REPOSITORY    https://github.com/FluidSynth/fluidsynth
  GIT_TAG           "v2.5.0"
  INSTALL_DIR       @DEPENDENCIES_INSTALL_DIR@
  CMAKE_ARGS        @DEPENDENCIES_CMAKE_DEFAULT_ARGS@ -DBUILD_SHARED_LIBS=ON -DCMAKE_INSTALL_PREFIX=<INSTALL_DIR> -Denable-openmp=off -Dosal=cpp11 -Denable-libinstpatch=0
)
